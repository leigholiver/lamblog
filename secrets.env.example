# [optional] your aws cli auth will be used by default
# (set the aws_profile environment var to specify)
export AWS_ACCESS_KEY_ID=
export AWS_SECRET_ACCESS_KEY=
export AWS_DEFAULT_REGION=

# cloudflare auth for dns
export TF_VAR_cloudflare_email=
export TF_VAR_cloudflare_apikey=
export TF_VAR_cloudflare_account_id=
export TF_VAR_cloudflare_zone=

# single ip address or cidr to allow staging access
export STAGING_ALLOWED_IP=

# secret for tokens
export JWT_SECRET=